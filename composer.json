{
    "name": "zf2-boiler-app/app-db",
    "license": "MIT",
    "type": "library",
    "description": "ZF2 BoilerApp Database module",
    "keywords": ["Zend Framework 2","ZF2 Boiler-App","database","doctrine"],
    "authors": [
        {
            "name": "Neilime",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zf2-boiler-app/app-db/issues"
    },
    "minimum-stability" : "dev",
    "require": {
        "php": ">=5.3.3",
        "ext-pdo_mysql": "*",
        "zendframework/zendframework": "2.2.*@dev",
        "doctrine/doctrine-orm-module": "0.8.*@dev"
    },
    "require-dev":{
    	"zf2-boiler-app/app-test": "1.0.*@dev"
    },
    "autoload": {
        "psr-0": {
            "BoilerAppDb": "src"
        },
        "classmap" : ["Module.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}